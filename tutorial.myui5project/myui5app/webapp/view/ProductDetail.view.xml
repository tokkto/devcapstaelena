<mvc:View controllerName="myui5app.controller.ProductDetail"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	displayBlock="true">
	<Page id="ProductDetail" title="Detail Page">
		<VBox>
			<Text text="{ProductName}"/>
			<Text text="{UnitPrice}"/>
			<Text text="{QuantityPerUnit}"/>
			<Text text="{UnitsInStock}"/>
		</VBox>
	</Page>
</mvc:View>
